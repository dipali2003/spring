<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    
    <bean class="in.sp.beans.Address" id="addrId">
     
     <property name="houseno" value="444"/>
     <property name="name" value=" DD house"/>
     <property name="pincode" value="112456"/>
    
    </bean>

   <bean class="in.sp.beans.student" id="stdId">
   <property name="rollno" value="102"/>
   <property name="name" value="mauli"/>
   <property name="address" ref="addrId"/>
   


  </bean>
   

</beans>